<h1>Listing calendar events</h1>
 
<table>
  <tr>
    <th>Title</th>
    <th>Launch Date</th>
    <th>Thumbnail</th>
  </tr>
 
  <% @calendars.each do |calendar| %>
    <tr>
      <td><%= calendar.title %></td>
      <td><%= calendar.launch_date.strftime("%m/%d/%Y")%></td>
      <td><%= image_tag calendar.thumbnail_url %></td>
      <td><%= link_to 'Show', edit_calendar_path(calendar) %></td>
    </tr>
  <% end %>
</table>